>>> # квадраты чисел от 0 до 9
... [x**2 for x in range(10)]
[0, 1, 4, 9, 16, 25, 36, 49, 64, 81]
>>> # квадраты четных чисел от 0 до 9:
... [x**2 for x in range(10) if x % 2 == 0]
[0, 4, 16, 36, 64]
>>> # кортеж
... (x**2 for x in range(10))
(0, 1, 4, 9, 16, 25, 36, 49, 64, 81)
>>> # словарь
... { "K" + str(i): i**2 for i in range(6)}
{'K0': 0, 'K1': 1, 'K2': 4, 'K3': 9, 'K4': 16, 'K5': 25, 'K6': 36}
>>>
